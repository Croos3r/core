    "git-ensure": "^0.1.0",
    "tsup": "^4.8.21",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@discord-factory/env": "^0.0.0",
    "@types/mock-fs": "^4.13.0",
    "cross-env": "^7.0.3",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "fs-recursive": "^1.0.6",
    "js-yaml": "^4.1.0",
    "mock-fs": "^4.14.0",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "yaml": "^1.10.2"
